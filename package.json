{
  "name": "vue3-scrolling-number",
  "version": "0.0.8",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "v": "npm version patch",
    "release": "standard-version --header",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "pub": "vite build && npm config set registry https://registry.npmjs.org && npm publish && npm config set registry https://registry.npmmirror.com"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "vue3-scrolling-number": "^0.0.8"
  },
  "bugs": {
    "url": "https://github.com/niboac/vue3-scroll-number/issues"
  },
  "homepage": "https://github.com/niboac/vue3-scroll-number#readme",
  "files": [
    "package.json",
    "dist",
    "src"
  ],
  "main": "./dist/vue3-scrolling-number.umd.js",
  "module": "./dist/vue3-scrolling-number.es.js",
  "exports": {
    ".": {
      "types": "./dist/vue3-scrolling-number.es.d.ts",
      "import": "./dist/vue3-scrolling-number.es.js",
      "require": "./dist/vue3-scrolling-number.umd.js"
    }
  },
  "types": "./dist/vue3-scrolling-number.es.d.ts",
  "keywords": [
    "vue3",
    "TypeScript",
    "scroll",
    "number",
    "vue3-scroll-number"
  ]
}